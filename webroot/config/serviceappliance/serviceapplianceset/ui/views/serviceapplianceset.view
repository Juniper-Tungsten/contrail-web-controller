<script type="text/x-handlebars-template" id="gridSapSetDetailTemplate">
    <div>
        <div class="row-fluid">
            <div class="row-fluid">
                <div class="span2"><label>Appliance Set Name</label></div>
                <div class="span10"> {{service_appliance_set_name}} </div>
            </div>
            <div class="row-fluid">
                <div class="span2"><label>UUID</label></div>
                <div class="span10"> {{uuid}} </div>
            </div>
            <div class="row-fluid">
                <div class="span2"><label>HA Mode</u></div>
                <div class="span10"> {{service_appliance_ha_mode}} </div>
            </div>
            <div class="row-fluid">
                <div class="span2"><label>Load balancer driver</u></div>
                <div class="span10"> {{service_appliance_driver}} </div>
            </div>
            <div class="row-fluid">
                <div class="span2"><label>Appliances</label></div>
                <div class="span10"> {{service_appliances}} </div>
            </div>
            <div class="row-fluid">
                <div class="span2"><label>Properties</label></div>
                <div class="span10"> {{sap_set_prop_string}}</div>
            </div>
        </div>
    </div>
</script>
<script type='text/x-handlebars-template' id='sapset-config-template'>
    <div id="windowCreateSapSet" class="modal modal-700 hide" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
	    <div class="modal-header">
	  	    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
	  	    <h6 class="modal-header-title"></h6>
	    </div>
  	    <div class="modal-body">
	        <form class="form-horizontal">
	            <div  class="control-group">
	                <label class="control-label" class="span3">Name</label>
	                <div class="controls">
	            	    <div class="row-fluid">
		                    <input type="text" id="txtSapSetName" class="span10" />
		                </div>
	                </div>
	            </div>
	            <div class="control-group">
	                <label class="control-label">Load Balancer Driver</label>
                    <div class="controls">
	                    <div class="row-fluid">
		                    <input type="text" id="cmbApplianceDriver" class="span10" />
		                </div>
	                </div>
	            </div>
	            <div class="control-group">
	                <label class="control-label">HA Mode</label>
                    <div class="controls">
	                    <div class="row-fluid">
		                    <input type="text" id="cmbHAMode" class="span10" />
		                </div>
	                </div>
	            </div>
                <div class="control-group">
                    <label class="control-label">Key Value Pairs</label>
                    <div class="controls">
                        <div class="row-fluid margin-0-0-10">
                            <div class="rule-item-header span5">
                                <div class="row-fluid">
                                    <div class="span11">
                                        <h6>Key Value Pair</h6>
                                    </div>
                                    <div class="span1 pull-left">
                                        <div title="Add Key Value Pair below" class="margin-5 padding-10-0">
                                            <i id="btnCommonAddKeyValuePair" class="pull-left icon-plus" onclick="appendKVEntry(this, true);"></i>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row-fluid">
                            <div id="kvTuples" class="row-fluid"></div>
                        </div>
                    </div>
                </div>
	        </form>
	    </div>
	    <div class="modal-footer">
	        <button id="btnCreateSapSetCancel" class="btn btn-mini" data-dismiss="modal" aria-hidden="true">Cancel</button>
	        <button id="btnCreateSapSetOK" class="btn btn-primary btn-mini">Save</button>
  	    </div>
    </div>
    
    <div id="confirmDelete" class="modal modal-420 hide" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
	    <div class="modal-header">
	  	    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
	  	    <h6 class="modal-header-title"></h6>
	    </div>
  	    <div class="modal-body">
            <div class="row-fluid text-center">
                <h6>Confirm Service Appliance Set(s) delete</h6>
            </div>       
        </div>
        <div class="modal-footer">
	        <button id="btnCnfDelPopupCancel" class="btn btn-mini" data-dismiss="modal" aria-hidden="true">Cancel</button>
	        <button id="btnCnfDelPopupOK" class="btn btn-primary btn-mini">Confirm</button>
  	    </div>
    </div>
    
    <div id="confirmDeleterow" class="modal modal-420 hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            <h6 class="modal-header-title"></h6>
        </div>
        <div class="modal-body">
            <div class="row-fluid text-center">
                <h6>Confirm Service Appliance Set delete</h6>
            </div>       
        </div>
        <div class="modal-footer">
            <button id="btnCnfDelRowPopupCancel" class="btn btn-mini" data-dismiss="modal" aria-hidden="true">Cancel</button>
            <button id="btnCnfDelRowPopupOK" class="btn btn-primary btn-mini">Confirm</button>
        </div>
    </div>
    
    <div class="row-fluid">
        <div class="span12">
            <div id="gridSapSet"/>
        </div>
    </div>
</script>
